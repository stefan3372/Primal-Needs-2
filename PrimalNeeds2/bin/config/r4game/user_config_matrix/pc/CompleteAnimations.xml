			</Preset>
		</PresetsArray>
		<VisibleVars>
			<Var id="CAeatsp"           displayName="CA_eatsp"           displayType="SLIDER;0;2;200"/> 
			<Var id="CAdrinksp" 	    displayName="CA_drinksp" 		 displayType="SLIDER;0;2;200"/> 
			<Var id="CAoilsp"           displayName="CA_oilsp"           displayType="SLIDER;0;2;200"/> 
			<Var id="CAgrindstonesp"    displayName="CA_grindstonesp"    displayType="SLIDER;0;2;200"/> 
			<Var id="CAworkbenchsp"     displayName="CA_workbenchsp"     displayType="SLIDER;0;2;200"/> 
			<Var id="CAlootstartsp"     displayName="CA_lootstartsp"     displayType="SLIDER;0;2;200"/> 
