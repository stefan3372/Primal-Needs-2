<?xml version="1.0" encoding="UTF-16"?>
<UserConfig>
	<Group id="CAanim"                  displayName="Mods.CA.toggles">
		<PresetsArray>
			<Preset id="0"              displayName="Default">
				<Entry varId="CAeat"                value="0.0" /> 
