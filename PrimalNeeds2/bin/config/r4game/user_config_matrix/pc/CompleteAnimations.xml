				<Entry varId="CAoilsp"              value="1.0" />
				<Entry varId="CAgrindstonesp"       value="1.0" />
				<Entry varId="CAworkbenchsp"        value="1.0" />
				<Entry varId="CAlootstartsp"        value="1.0" />
