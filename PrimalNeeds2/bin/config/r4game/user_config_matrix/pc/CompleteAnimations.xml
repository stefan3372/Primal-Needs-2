				<OptionsArray>
					<Option id="0" displayName="CA_all_loot">
                        <Entry varId="CAloot" value="0"/>
                    </Option>
					<Option id="1" displayName="CA_only_herb">
                        <Entry varId="CAloot" value="1"/>
                    </Option>
					<Option id="2" displayName="CA_loot_alt">
                        <Entry varId="CAloot" value="2"/>
                    </Option>
					<Option id="3" displayName="CA_loot_alt_no_herb">
                        <Entry varId="CAloot" value="3"/>
                    </Option>
					<Option id="4" displayName="CA_disabled">
                        <Entry varId="CAloot" value="4"/>
                    </Option>
				</OptionsArray>
			</Var>
			<Var id="CAgrindstone"      displayName="CA_grindstone"      displayType="OPTIONS">
