				<Entry varId="CAoil"                value="1.0" /> 
				<Entry varId="CAloot"               value="0.0" /> 
				<Entry varId="CAgrindstone"         value="1.0" /> 
				<Entry varId="CAworkbench"          value="1.0" />
			</Preset>
		</PresetsArray>
		<VisibleVars>
			<Var id="CAeat"             displayName="CA_eat"             displayType="OPTIONS">
				<OptionsArray>
					<Option id="0" displayName="CA_movable">
                        <Entry varId="CAeat" value="0"/>
                    </Option>
                    <Option id="1" displayName="CA_static_default">
                        <Entry varId="CAeat" value="1"/>
