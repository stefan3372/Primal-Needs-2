					<Option id="5" displayName="CA_disabled">
                        <Entry varId="CAeat" value="5"/>
                    </Option>
				</OptionsArray>
			</Var>
			<Var id="CAdrink"           displayName="CA_drink"           displayType="OPTIONS">
